package IGGUI;

/**
 *
 * @author AltonJr
 */
public class Payment {
    
    private float salePrice;
    private float totalPrice;
    
    public float getSalePrice(Menu item)
    {
        return salePrice;
    }
    
    public float totalPrice(float salePrice, int qty)
    {
        totalPrice=(salePrice*qty);
        return totalPrice;
    }
}
